{
  "name": "interactive-node-samples",
  "version": "1.0.0",
  "description": "A collection of samples for the interactive-node SDK",
  "contributors": [
    {
      "name": "Mike Blouin",
      "email": "mb@xbox.com"
    }
  ],
  "scripts": {
    "fmt:javascript": "prettier --single-quote --trailing-comma es5 --print-width 100 --write \"**/*.js\"",
    "fmt:typescript": "prettier --single-quote --trailing-comma all --print-width 100 --parser typescript --write \"**/*.{ts,tsx}\"",
    "fmt": "npm run fmt:javascript && npm run fmt:typescript",
    "lint:typescript": "tslint -c tslint.json --exclude=\"**/node_modules/**/*.ts\" \"**/*.ts\"",
    "lint": "npm run lint:typescript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixer/interactive-node-samples.git"
  },
  "keywords": [
    "Interactive",
    "Mixer"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixer/interactive-node-samples/issues"
  },
  "homepage": "https://github.com/mixer/interactive-node-samples#readme",
  "devDependencies": {
    "prettier": "^1.4.4",
    "tslint": "^4.3.1",
    "tslint-microsoft-contrib": "4.0.0",
    "typescript": "^2.7.2"
  }
}
